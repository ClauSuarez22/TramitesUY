<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uy.edu.ucu.android.tramitesuy" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".controller.Login"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver
            android:name=".controller.MyBroadcastReceiver"
            android:label="@string/title_activity_my_broadcast_receiver"
            android:parentActivityName=".controller.Login" >
            <meta-
                android:name="android.support.PARENT_ACTIVITY"
                android:value="uy.edu.ucu.android.tramitesuy.controller.Login" />
        </receiver>
        <activity
            android:name=".controller.MyIntentService"
            android:label="@string/title_activity_my_intent_service"
            android:parentActivityName=".controller.Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="uy.edu.ucu.android.tramitesuy.controller.Login" />
        </activity>
        <activity
            android:name=".controller.ResultsActivity"
            android:label="@string/title_activity_results"
            android:parentActivityName=".controller.Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="uy.edu.ucu.android.tramitesuy.controller.Login" />
        </activity>
    </application>

</manifest>
